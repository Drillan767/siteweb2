<%= simple_form_for(@article, html: {multipart: true}) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :titre %>
    <%= f.input :resume %>
    <%= f.input :contenu %>
    <%= f.label :image %>
    <%= f.file_field :image %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
