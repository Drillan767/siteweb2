<h1>Contact</h1>
<%= form_for (@contact), :html => { :class => 'class' } do |f| %>

    <%= f.label :nom %>
    <%= f.text_field :nom %>
    <%= f.label :email %>
    <%= f.text_field :email %>
    <%= f.label :objet %>
    <%= f.text_field :objet %>
    <%= f.label :message %>
    <%= f.text_area :message %>
    <%= f.submit 'Envoyer' %>
<% end %>